RewriteRule    ^cricket-news/([0-9a-zA-Z_-]+)/([0-9]+)    single-post.php?url=$1&postid=$2 
RewriteRule    ^full-score/([0-9a-zA-Z_-]+)/([0-9]+)    single-scorecard.php?url=$1&postid=$2

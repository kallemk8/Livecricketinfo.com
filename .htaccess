RewriteRule    ^player/([0-9a-zA-Z_-]+)/([0-9]+)    single-player.php?url=$1&postid=$2
 

RewriteRule    ^category/([0-9a-zA-Z_-]+)/([0-9]+)    category.php?catname=$1&postid=$2  

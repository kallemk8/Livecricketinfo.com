
RewriteEngine On    # Turn on the rewriting engine
 
RewriteRule    ^([0-9a-zA-Z_-]+)/([0-9]+)    single.php?url=$1&postid=$2 
RewriteRule    ^news/([0-9a-zA-Z_-]+)/([0-9]+)    contentsingle.php?url=$1&postid=$2 
RewriteRule    ^cricket-series/([0-9a-zA-Z_-]+)/([0-9]+)    single-series.php?url=$1&postid=$2 
RewriteRule    ^cricket-videos/([0-9a-zA-Z_-]+)/([0-9]+)    single-video.php?url=$1&postid=$2 
